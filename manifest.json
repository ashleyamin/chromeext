{
  "manifest_version": 2,
  "name": "TestExt",
  "description": "Practice building a chrome extension.",
  "version": "1",
  "author": "Ashley Amin",

  "browser_action": {
    "default_icon": "tab-icon.png",
    "default_title": "Have a good day"
  },

  "chrome_url_overrides": {
    "newtab": "newtab.html"
  },

  "permissions": ["activeTab"]
}
